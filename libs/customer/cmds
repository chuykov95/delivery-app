# Создаем заново без buildable
nx g @nx/angular:lib shared/ui --standalone
nx g @nx/angular:lib shared/data-access
nx g @nx/angular:lib shared/utils
nx g @nx/angular:lib shared/models
nx g @nx/angular:lib shared/guards
nx g @nx/angular:lib shared/pipes

